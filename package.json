    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "seed": "psql -h ec2-18-235-97-230.compute-1.amazonaws.com -d deu06qntv5kd9c -U ccjqyyvqrtrixn -f seeds/seeds.sql"
  },
