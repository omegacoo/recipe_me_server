    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.11",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "xss": "^1.0.6",
    "minimist": ">=0.2.1"
  },
